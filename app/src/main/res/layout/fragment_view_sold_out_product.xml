<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/back_colour_view_sold_out_product"
        android:layout_width="356dp"
        android:layout_height="641dp"
        android:background="#92FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.655" />

    <TextView
        android:id="@+id/textview_price_view_sold_out_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="価格："
        android:textColor="@color/red_brown"
        android:textSize="34sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintEnd_toEndOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintTop_toTopOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintVertical_bias="0.501" />

    <TextView
        android:id="@+id/area_view_sold_out_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="地域　　："
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintEnd_toEndOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintTop_toTopOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintVertical_bias="0.576" />

    <TextView
        android:id="@+id/listing_day_view_sold_out_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="出品日　："
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintEnd_toEndOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintTop_toTopOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintVertical_bias="0.614" />

    <ImageView
        android:id="@+id/product_image_view_sold_out_product"
        android:layout_width="133dp"
        android:layout_height="148dp"
        app:layout_constraintBottom_toBottomOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintEnd_toEndOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintVertical_bias="0.233"
        app:srcCompat="@drawable/ic_launcher_foreground" />
    <Button
        android:id="@+id/delivery_procedure_button_view_sold_out_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F2BB05"
        android:text="配送手続き"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintEnd_toEndOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintTop_toTopOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintVertical_bias="0.806" />

    <TextView
        android:id="@+id/product_name_text_view_view_sold_out_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="商品名"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintEnd_toEndOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintHorizontal_bias="0.169"
        app:layout_constraintStart_toStartOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintTop_toTopOf="@+id/back_colour_view_sold_out_product"
        app:layout_constraintVertical_bias="0.116" />

</androidx.constraintlayout.widget.ConstraintLayout>