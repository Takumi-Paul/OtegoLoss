<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmentViewInputShippingAddress"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2E8CF" >

    <ImageView
        android:id="@+id/backGround2_imageView"
        android:layout_width="410dp"
        android:layout_height="684dp"
        app:srcCompat="@drawable/background"
        tools:layout_editor_absoluteX="-15dp"
        tools:layout_editor_absoluteY="4dp" />

    <Button
        android:id="@+id/addAddress_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="住所を追加"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="449dp" />

    <Button
        android:id="@+id/deleteAddress_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="住所を削除"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="506dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?android:attr/windowBackground"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <View
        android:id="@+id/shippingAddress1_view"
        android:layout_width="338dp"
        android:layout_height="150dp"
        android:background="#92FFFFFF"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="66dp" />

    <View
        android:id="@+id/shippingAddress2_view"
        android:layout_width="338dp"
        android:layout_height="150dp"
        android:background="#92FFFFFF"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="249dp" />

    <RadioButton
        android:id="@+id/choice1_radioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="123dp" />

    <RadioButton
        android:id="@+id/choice2_radioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="303dp" />

    <TextView
        android:id="@+id/addressNumber1_textView"
        android:layout_width="104dp"
        android:layout_height="25dp"
        android:text="郵便番号1"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="86dp" />

    <TextView
        android:id="@+id/addressNumber2_textView"
        android:layout_width="104dp"
        android:layout_height="25dp"
        android:text="郵便番号2"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="269dp" />

    <TextView
        android:id="@+id/address1_textView"
        android:layout_width="90dp"
        android:layout_height="27dp"
        android:text="住所1"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="130dp" />

    <TextView
        android:id="@+id/address2_textView"
        android:layout_width="90dp"
        android:layout_height="27dp"
        android:text="住所2"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="312dp" />

    <TextView
        android:id="@+id/name1_textView"
        android:layout_width="90dp"
        android:layout_height="27dp"
        android:text="名前1"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="171dp" />

    <TextView
        android:id="@+id/name2_textView"
        android:layout_width="90dp"
        android:layout_height="27dp"
        android:text="名前2"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="352dp" />

</androidx.constraintlayout.widget.ConstraintLayout>