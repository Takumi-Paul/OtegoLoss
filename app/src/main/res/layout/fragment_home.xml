<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmentHome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2E8CF" >

    <ImageView
        android:id="@+id/backGround2_imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/nav_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background" />

    <ImageView
        android:id="@+id/productImage_imageView_home4"
        android:layout_width="159dp"
        android:layout_height="125dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.849"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backGround_view3"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/productImage_imageView_home3"
        android:layout_width="159dp"
        android:layout_height="125dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backGround_view2"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/productImage_imageView_home2"
        android:layout_width="159dp"
        android:layout_height="125dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/backGround2_imageView"
        app:srcCompat="@mipmap/ic_launcher" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?android:attr/windowBackground"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu" >

    </com.google.android.material.bottomnavigation.BottomNavigationView>

    <View
        android:id="@+id/backGround_view2"
        android:layout_width="160dp"
        android:layout_height="65dp"
        android:layout_marginTop="4dp"
        android:background="#92FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productImage_imageView_home1" />

    <View
        android:id="@+id/backGround_view4"
        android:layout_width="160dp"
        android:layout_height="65dp"
        android:layout_marginTop="12dp"
        android:background="#92FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productImage_imageView_home3" />

    <View
        android:id="@+id/backGround_view5"
        android:layout_width="160dp"
        android:layout_height="65dp"
        android:layout_marginTop="16dp"
        android:background="#92FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.844"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productImage_imageView_home4" />

    <View
        android:id="@+id/backGround_view3"
        android:layout_width="160dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:background="#92FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productImage_imageView_home2" />

    <TextView
        android:id="@+id/product_name_home4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="商品名4"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view5"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@+id/backGround_view5"
        app:layout_constraintTop_toTopOf="@+id/backGround_view5" />

    <TextView
        android:id="@+id/product_name_home3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="商品名3"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view4"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="@+id/backGround_view4"
        app:layout_constraintTop_toTopOf="@+id/backGround_view4" />

    <TextView
        android:id="@+id/product_name_home1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="商品名1"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view2"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="@+id/backGround_view2"
        app:layout_constraintTop_toTopOf="@+id/backGround_view2" />

    <TextView
        android:id="@+id/product_name_home2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="商品名2"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view3"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="@+id/backGround_view3"
        app:layout_constraintTop_toTopOf="@+id/backGround_view3" />

    <TextView
        android:id="@+id/sellar_id_home2"
        android:layout_width="64dp"
        android:layout_height="20dp"
        android:layout_marginTop="8dp"
        android:text="出品者ID2"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view3"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="@+id/backGround_view3"
        app:layout_constraintTop_toBottomOf="@+id/product_name_home2" />

    <TextView
        android:id="@+id/sellar_id_home1"
        android:layout_width="64dp"
        android:layout_height="20dp"
        android:layout_marginTop="8dp"
        android:text="出品者ID1"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view2"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="@+id/backGround_view2"
        app:layout_constraintTop_toBottomOf="@+id/product_name_home1" />

    <TextView
        android:id="@+id/sellar_id_home3"
        android:layout_width="64dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dp"
        android:text="出品者ID3"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view4"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="@+id/backGround_view4"
        app:layout_constraintTop_toBottomOf="@+id/product_name_home3" />

    <TextView
        android:id="@+id/sellar_id_home4"
        android:layout_width="64dp"
        android:layout_height="20dp"
        android:layout_marginTop="8dp"
        android:text="出品者ID4"
        app:layout_constraintEnd_toEndOf="@+id/backGround_view5"
        app:layout_constraintHorizontal_bias="0.104"
        app:layout_constraintStart_toStartOf="@+id/backGround_view5"
        app:layout_constraintTop_toBottomOf="@+id/product_name_home4" />

    <ImageButton
        android:id="@+id/productImage_imageView_home1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/backGround2_imageView"
        app:srcCompat="@drawable/ic_launcher_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>